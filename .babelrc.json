{
  "presets": ["next/babel"],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "babel-plugin-prismjs",
      {
        "languages": [
          "bash",
          "css",
          "docker",
          "erb",
          "javascript",
          "json",
          "jsx",
          "makefile",
          "ruby",
          "scss",
          "tsx",
          "typescript",
          "yaml"
        ]
      }
    ]
  ]
}
